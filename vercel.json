{
    "version": 2,
    "builds": [
      {
        "use": "@vercel/python",
        "entrypoint": "panel serve app.py --port $PORT --address 0.0.0.0",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ]
  }